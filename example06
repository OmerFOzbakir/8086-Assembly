TITLE PROGRAM3_3 CMP EX
.MODEL SMALL
.STACK 64

.DATA
GRADES  DB 69,87,96,45,75
HIGHEST DB ?

.CODE
MAIN PROC FAR
    MOV AX,@DATA
    MOV DS,AX
    MOV CX,5
    MOV BX, OFFSET GRADES
    SUB AL,AL
AGAIN: 
    CMP AL,[BX]
    JA  NEXT
    MOV AL,[BX]
NEXT:  
    INC BX
    LOOP AGAIN
    MOV HIGHEST, AL
    MOV AH,4ch
    INT 21h
MAIN ENDP
END MAIN
