TITLE PROG3-1B (EXE) ADDING 5 WORDS
PAGE 60,132
.MODEL SMALL
.STACK 64

.DATA
COUNT EQU 05
DATA  DW  27345, 28521, 29533, 30105, 32375
      ORG 0010H
SUM   DW  2 DUP(?)

.CODE
MAIN PROC FAR
    MOV AX,@DATA
    MOV DS,AX
    MOV CX,COUNT
    MOV SI,OFFSET DATA
    MOV AX,00
    MOV BX,AX
BACK: 
    ADD AX,[SI]
    ADC BX,0
    INC SI
    INC SI
    DEC CX
    JNZ BACK
    MOV SUM,AX
    MOV SUM+2,BX
    MOV AH,4CH
    INT 21H
MAIN ENDP
END MAIN
