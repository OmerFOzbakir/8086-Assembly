TITLE PROG3-1A (EXE) ADDING 5 BYTES
PAGE 60,132
.MODEL SMALL
.STACK 64

.DATA
COUNT EQU 05
DATA  DB  125,235,197,91,48
      ORG 0008H
SUM   DW  ?

.CODE
MAIN PROC FAR
    MOV AX,@DATA
    MOV DS,AX
    MOV CX,COUNT
    MOV SI,OFFSET DATA
    MOV AX,00
BACK: 
    ADD AL,[SI]
    JNC OVER
    INC AH
OVER:
    INC SI
    DEC CX
    JNZ BACK
    MOV SUM,AX
    MOV AH,4CH
    INT 21H
MAIN ENDP
END MAIN
